{
  "name": "list2tree-vscode-extension",
  "displayName": "list2tree",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "list2tree.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "check": "concurrently -c auto 'npm:check:*'",
    "check:lint": "biome lint **/*",
    "check:format": "treefmt --fail-on-change",
    "fix": "concurrently -c auto 'npm:check:*'",
    "fix:lint": "biome lint --apply **/*",
    "fix:format": "treefmt",
    "test": "pnpm run compile-tests && pnpm run compile && vscode-test"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@swc/register": "^0.1.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "concurrently": "^8.2.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
